<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity"
    android:id="@+id/f02"
    android:background="@drawable/w01"
     >

    <ImageView
        android:id="@+id/wmain01"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:src="@drawable/fon21" />

    <ImageView
        android:id="@+id/wmain04"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:src="@drawable/fon21" />

    <ImageView
        android:id="@+id/wmain03"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignTop="@+id/wmain04"
        android:layout_marginRight="30dp"
        android:layout_toLeftOf="@+id/wmain04"
        android:src="@drawable/fon21" />

    <ImageView
        android:id="@+id/wmain02"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignTop="@+id/wmain01"
        android:layout_marginLeft="30dp"
        android:layout_toRightOf="@+id/wmain01"
        android:src="@drawable/fon21" />

    <ImageView
        android:id="@+id/wkarta01"
        android:layout_width="fill_parent"
        android:layout_height="300dp"
        android:layout_alignLeft="@+id/wmain01"
        android:layout_alignParentTop="true"
        android:layout_marginTop="64dp"
        android:src="@drawable/fon21" />

    <ImageView
        android:id="@+id/wrezhim01"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:src="@drawable/fon21" />

    <ImageView
        android:id="@+id/wmess01"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_above="@+id/wmain01"
        android:layout_alignLeft="@+id/wmain01"
        android:src="@drawable/fon21" />

    <ImageView
        android:id="@+id/wactmar01"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:src="@drawable/fon21" />
    
</RelativeLayout>
